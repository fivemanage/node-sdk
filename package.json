{
  "name": "@fivemanage/sdk",
  "description": "Node.js utilities for Fivemanage",
  "version": "0.0.2",
  "author": "Fivemanage",
  "repository": {
    "type": "git",
    "url": "https://github.com/fivemanage/node-sdk"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": ["dist", "README.md", "package.json"],
  "scripts": {
    "build": "pnpm rimraf dist && tsup",
    "dev": "pnpm run build && node dist/index.js",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest watch",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "changeset": "changeset",
    "build:prepare": "pnpm run typecheck && pnpm run lint && pnpm run format && pnpm run check && pnpm run build && pnpm run test:coverage",
    "dev:prepare": "git pull origin main && pnpm i"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "2.1.4",
    "husky": "^9.1.6",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "packageManager": "pnpm@9.12.2",
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5"
  }
}
